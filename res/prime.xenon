#lib res/stdlib.xenon

int main() { 
    mut val a = 2 
    while a != 100 {
        mut val isPrime = true
        mut val i = 2
        while i != a {
             if ((a/i)*i) == a {
                 isPrime = false
             }
             i = i + 1
        }
        if isPrime {
            println(a) 
        }
        a = a + 1
    }
    return 0
}

@pure bool isDivisableBy(int a, int b) {
    if(a == b) { return true }
    return ((a/b) * b) == a
}

