
struct number_container {
    long index
}
struct number {
    int number
    number_container cont
}


long getIndex(number n){
    return n.number
}

int main() {
    //val x = 8
    val x = number{}
    val xValPtr = &x.number
    *xValPtr = 7
    x.cont.index = 101l
    println(x.number)
    println(x.cont.index)
    println(getIndex(x))


    return 0

}







